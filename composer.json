{
    "name": "xboston/metahash",
    "description": "Pure PHP metahash library",
    "keywords": ["metahash", "blockchain"],
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "mdanter/ecc": "^0.5.2"
    },
    "autoload": {
        "psr-4": {
            "Metahash\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "overtrue/phplint": "^1.1",
        "phpstan/phpstan": "^0.11.4",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
